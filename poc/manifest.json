{
  "manifest_version": 2,

  "name": "chrome-extension-url-mapper",
  "description": "A Chrome extension to map URL",
  "version": "0.1",

  "browser_action": {
    "default_icon": "1452302928_10.Folder.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
